{
  "version": 2,
  "builds": [
    {
      "src": "acm_website/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/index.ts" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "acm_website/dist/index.html" }
  ]
}
