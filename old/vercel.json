{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "acm_website/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "acm_website/dist/$1"
    }
  ],
  "functions": {
    "api/index.ts": {
      "includeFiles": "shared/**"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
