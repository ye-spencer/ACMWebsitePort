{
  "version": 2,
  "builds": [
    {
      "src": "acm_website/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "src/server.ts" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "acm_website/dist/index.html" }
  ]
}
